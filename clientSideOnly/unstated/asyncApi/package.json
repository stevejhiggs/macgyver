{
  "name": "simplecomponentts",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "isomorphic-fetch": "^3.0.0",
    "koa": "^2.13.4",
    "koa-bodyparser": "^4.3.0",
    "koa-router": "^10.1.1",
    "koa-static": "^5.0.0",
    "koa2-cors": "^2.0.6",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "unstated": "^2.1.1"
  },
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@types/koa": "^2.13.4",
    "@types/koa-bodyparser": "^5.0.0",
    "@types/koa-router": "^7.4.4",
    "@types/koa-static": "^4.0.2",
    "@types/koa2-cors": "^2.0.2",
    "@types/node": "^16.11.7",
    "@types/react": "^17.0.34",
    "@types/react-dom": "^17.0.11",
    "concurrently": "^6.4.0",
    "react-scripts": "4.0.3",
    "ts-node": "^10.4.0",
    "tslint": "^5.20.1",
    "typescript": "^4.4.4"
  },
  "scripts": {
    "start": "concurrently --kill-others \"node ./src/server/index.js\" \"react-scripts start\"",
    "prebuild": "rm -rf .dist",
    "build": "concurrently --kill-others-on-fail \"react-scripts build\" \"tsc -p ./src/server/tsconfig.json --outDir .dist\"",
    "postbuild": "mkdir -p .dist && mv build .dist/public",
    "pretest": "tslint -p ./server/tsconfig.json -t stylish",
    "test": "react-scripts test --env=jsdom",
    "runProduction": "node ./.dist/server.js"
  },
  "proxy": "http://localhost:8000/",
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
