{
  "name": "simplecomponentts",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "isomorphic-fetch": "^2.2.1",
    "koa": "^2.7.0",
    "koa-bodyparser": "^4.2.1",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "koa2-cors": "^2.0.6",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "unstated": "^2.1.1"
  },
  "devDependencies": {
    "@types/jest": "^24.0.11",
    "@types/koa": "^2.0.48",
    "@types/koa-bodyparser": "^5.0.0",
    "@types/koa-router": "^7.0.40",
    "@types/koa-static": "^4.0.1",
    "@types/koa2-cors": "^2.0.1",
    "@types/node": "^11.13.4",
    "@types/react": "^16.8.13",
    "@types/react-dom": "^16.8.4",
    "concurrently": "^4.1.0",
    "react-scripts": "2.1.8",
    "ts-node": "^8.0.3",
    "tslint": "^5.15.0",
    "typescript": "^3.4.3"
  },
  "scripts": {
    "start": "concurrently --kill-others \"node ./src/server/index.js\" \"react-scripts start\"",
    "prebuild": "rm -rf .dist",
    "build": "concurrently --kill-others-on-fail \"react-scripts build\" \"tsc -p ./src/server/tsconfig.json --outDir .dist\"",
    "postbuild": "mkdir -p .dist && mv build .dist/public",
    "pretest": "tslint -p ./server/tsconfig.json -t stylish",
    "test": "react-scripts test --env=jsdom",
    "runProduction": "node ./.dist/server.js"
  },
  "proxy": "http://localhost:8000/",
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
