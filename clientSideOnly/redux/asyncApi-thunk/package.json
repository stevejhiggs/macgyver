{
  "name": "simplecomponentts",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "isomorphic-fetch": "^2.2.1",
    "koa": "^2.5.1",
    "koa-bodyparser": "^4.2.1",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "koa2-cors": "^2.0.5",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-redux": "^5.1.0",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "typescript-fsa": "2.5.0",
    "typescript-fsa-reducers": "^1.0.0",
    "typescript-fsa-redux-thunk": "^2.0.0-beta.11"
  },
  "devDependencies": {
    "@types/jest": "^23.3.9",
    "@types/koa": "^2.0.46",
    "@types/koa-bodyparser": "^5.0.0",
    "@types/koa-router": "^7.0.30",
    "@types/koa-static": "^4.0.0",
    "@types/koa2-cors": "^2.0.1",
    "@types/node": "^10.12.2",
    "@types/react": "^16.4.18",
    "@types/react-dom": "^16.0.9",
    "@types/react-redux": "^6.0.9",
    "concurrently": "^3.6.0",
    "react-scripts": "2.1.1",
    "redux-devtools-extension": "^2.13.5",
    "ts-node": "^7.0.0",
    "tslint": "^5.10.0",
    "typescript": "^3.1.6"
  },
  "scripts": {
    "start": "concurrently --kill-others \"node ./src/server/index.js\" \"react-scripts start\"",
    "prebuild": "rm -rf .dist",
    "build": "concurrently --kill-others-on-fail \"react-scripts build\" \"tsc -p ./src/server/tsconfig.json --outDir .dist\"",
    "postbuild": "mkdir -p .dist && mv build .dist/public",
    "pretest": "tslint -p ./server/tsconfig.json -t stylish",
    "test": "react-scripts test --env=jsdom",
    "runProduction": "node ./.dist/server.js"
  },
  "proxy": "http://localhost:8000/",
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
